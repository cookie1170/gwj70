; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot wild jam 70"
run/main_scene="res://scenes/levels/woods.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.0784314, 0.133333, 0.25098, 1)
boot_splash/image="res://boot splash.png"
config/icon="res://gwj icon.svg"

[autoload]

gm="*res://scripts/gm.gd"
transition="*res://scenes/reusable/transition.tscn"
GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"narrator": "res://dialogic/narrator.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"first_enemy": "res://dialogic/first_enemy.dtl",
"woods": "res://dialogic/woods.dtl",
"woods_2": "res://dialogic/woods_2.dtl",
"woods_3": "res://dialogic/woods_3.dtl",
"woods_4": "res://dialogic/woods_4.dtl"
}
text/letter_speed=0.05
text/initial_text_reveal_skippable=false
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0.3
choices/reveal_by_input=true
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={}
layout/style_list=["res://dialogic/narrator_style.tres"]
layout/default_style="res://dialogic/narrator_style.tres"
glossary/default_case_sensitive=true
text/input_action="dialogic_default_action"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/simple-gui-transitions/plugin.cfg")

[gui_transitions]

config/default/auto_start=true
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=0.5
config/default/delay=0.5
config/default/transition_type="QUAD"
config/default/ease_type="IN_OUT"
config/default/animation_enter=4
config/default/animation_leave=4

